{
  "name": "webvi-web-bluetooth",
  "version": "1.0.0",
  "description": "An experimental WebVI JSLI wrapper for the Web Bluetooth API",
  "main": "source/main.js",
  "scripts": {
    "test": "npm run lint && npm run spell && npm run links",
    "lint": "eslint ./source/**/*.js",
    "spell": "mdspell --en-us --ignore-numbers \"./*.md\" --report",
    "links": "glob-exec --foreach \"./*.md\" -- \"markdown-link-check -c .linkcheck.json {{file}}\"",
    "serve": "http-server . -p 8040 -o http://localhost:8040/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajsite/webvi-web-bluetooth.git"
  },
  "keywords": [
    "WebVI",
    "LabVIEW",
    "NXG",
    "JSLI"
  ],
  "author": "Milan Raj (@rajsite)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajsite/webvi-web-bluetooth/issues"
  },
  "homepage": "https://github.com/rajsite/webvi-web-bluetooth#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "glob-exec": "^0.1.1",
    "http-server": "^0.11.1",
    "markdown-link-check": "^3.7.2",
    "markdown-spellcheck": "^1.3.1"
  },
  "dependencies": {}
}
